{
  "name": "Mindnote",
  "version": "0.0.5",
  "description": "A chrome extension which quickly collect idea and knowledge on web",
  "manifest_version": 2,
  "icons": {
    "16": "/imgs/16x16.png",
    "48": "/imgs/48x48.png",
    "120": "/imgs/120x120.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["declarativeContent", "storage", "https://*/*", "identity", "clipboardRead", "tabs"],
  "web_accessible_resources": [
    "fontawesome-free-5.9.0-web/css/all.css",
    "util/extended-prototype.js",
    "util/debounce.js",
    "components/board.js",
    "components/node.js",
    "page/dialog.html"
  ],
  "commands": {
    "dialog": {
      "suggested_key": {
        "windows": "Ctrl+Shift+P",
        "mac": "Command+Shift+P",
        "chromeos": "Ctrl+Shift+P",
        "linux": "Ctrl+Shift+P"
      },
      "description": "popup dialog"
    },
    "switch_to_board": {
      "suggested_key": {
        "windows": "Ctrl+Shift+1",
        "mac": "Command+Shift+1",
        "chromeos": "Ctrl+Shift+1",
        "linux": "Ctrl+Shift+1"
      },
      "description": "switch to board"
    },
    "switch_to_history": {
      "suggested_key": {
        "windows": "Ctrl+Shift+2",
        "mac": "Command+Shift+2",
        "chromeos": "Ctrl+Shift+2",
        "linux": "Ctrl+Shift+2"
      },
      "description": "switch to history"
    },
    "unshift_to_clipboard": {
      "suggested_key": {
        "windows": "Ctrl+Shift+O",
        "mac": "Command+Shift+O",
        "chromeos": "Ctrl+Shift+O",
        "linux": "Ctrl+Shift+O"
      },
      "description": "unshift to clipboard"
    }
  },
  "oauth2": {
    "client_id": "675953765772-uvdlm01kmotkh11onjv22r2rja3eoeio.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "content_security_policy": "script-src 'self' https://sapiens.tools; object-src 'self'"
}